case "$nxt_module" in

   python)
       . auto/modules/python
   ;;

   php)
       . auto/modules/php
   ;;

   go)
       . auto/modules/go
   ;;

   perl)
       . auto/modules/perl
   ;;

   ruby)
       . auto/modules/ruby
   ;;

   nodejs)
       . auto/modules/nodejs
   ;;

   java)
       . auto/modules/java
   ;;

   *)
       echo
       echo $0: error: invalid module \"$nxt_module\".
       echo
       exit 1
   ;;

esac
